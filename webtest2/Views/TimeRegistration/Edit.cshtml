@model IList<webtest2.ViewModels.TimeRegistrationViewModel>

    @Html.AntiForgeryToken()

    <h4>TimeRegistration</h4>
    <hr />
    <div class="container">
        <div class="row">
            <div class="col-md-2">Project Name</div>
            <div class="col-md-2">Time</div>
            <div class="col-md-2">Date</div>
        </div>
        @foreach (var reg in @Model)
        {
            @Html.Partial("TimeRegistration", reg)
        }
    </div>

    @using (Html.BeginForm())
    {
        <div class="form-inline">
            <div class="form-group">
                <div class="input-group">
                    <label>Create new Project</label>
                    <input type="text" class="form-control" name="ProjectName" />
                        <input type="submit" class="btn btn-default" value="Create" name="CreateProject" />
                </div>
            </div>
        </div>
    }
